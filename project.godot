; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ikun"
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")

[autoload]

ParticlesCache="*res://auto/particles_cache.gd"
SaverLoader="*res://auto/saver_loader/saver_loader.tscn"
DatatableManager="*res://auto/datatable_manager/datatable_manager.tscn"
SceneManager="*res://auto/scene_manager/scene_manager.gd"
ProcedureManager="*res://auto/procedure_manager/procedure_manager.tscn"
ChallengeService="*res://auto/challenge_service/challenge_service.tscn"
BackpackManager="*res://auto/backpack_manager.gd"
SettingManager="*res://auto/setting_manager/setting_manager.tscn"
Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"

[debug]

gdscript/warnings/unused_parameter=0

[dialogic]

directories/dch_directory={
"store_character": "res://dialogues/store_character.dch"
}
directories/dtl_directory={
"store": "res://dialogues/store.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://dialogues/style.tres"]
layout/default_style="res://dialogues/style.tres"
variables={}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
extensions_folder="res://addons/dialogic_additions"
translation/enabled=false
translation/original_locale="zh_CN"
translation/file_mode=1
translation/translation_folder=""
translation/save_mode=0

[dialogue_manager]

general/wrap_lines=true

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="ikun"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://themes/default_theme.tres"

